      "api": {
        "url": "${ls:backend_url}/messageTemplate/save",
        "method": "post",
        "requestAdaptor": "let jsonobj = {}; \n\n let content = json.parse (api.data.msgContent) .Content \n let url = json.parse (api.data.msgcontent) .url \n let title = json.parse (api.data.msgContent) .title \n let sendType = json.parse (api.data.msgContent) .sendtype \n let picurl = json.parse (api.data.msgcontent) .picurl \n\n\n // push notification bar \n if.data.sendchannel == '20') {{ \n     jsonObj.Content = Content \n     jsonobj.url = url \n     jsonObj.title = Title \n } \n // Short message \n if.data.sendchannel == '30') {{ \n     jsonobj.url = url \n     jsonObj.Content = Content \n } \n // mail \n if.data.sendchannel == '40') {{ \n     jsonObj.Content = Content \n     jsonObj.title = Title \n } \n // Enterprise WeChat (Application Message) \n if.data.sendchannel == '70') {{ \n     // Text, Markdown Type \n     if (Sendtype == '10' || Sendtype == '80') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n     } \n     // Image, video, voice, file type \n     if (Sendtype == '20' || Sendtype == '30' || Sendtype == '60' || Sendtype == '100') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.Mediaid = API.Data.Mediaid \n     } \n     // Text card type \n     if (seeType == '50') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.title = Title \n         jsonobj.url = url \n         jsonObj.Descript = API.Data.Descripting \n     } \n     // graphic type \n     if (seeType == '40') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.artICLES = json.stringify (API.Data.ARTICLES) \n     } \n\n } \n // Enterprise WeChat (robot) \n if.data.sendchannel == '100') {{ \n     // Text type \n     if (seeType == '10') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n     } \n     // Markdown type \n     if (seeType == '80') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n     } \n      // Image type \n     if (seeType == '100') { \n         jsonObj.md5 = api.data.md5 \n         jsonObj.base64 = api.data.base64 \n         jsonObj.SENDTYPE = Sendtype \n     } \n      // file type \n     if (seeType == '60') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.Mediaid = API.Data.Mediaid \n     } \n     // graphic type \n     if (seeType == '40') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.artICLES = json.stringify (API.Data.ARTICLES) \n     } \n } \n // WeChat service number \n if.data.sendchannel == '50') {{ \n     jsonobj.path = api.data.path \n     jsonObj.miniProgramid = API.Data.miniprogramid \n     jsonObj.templateid = api.data.templateid \n     jsonobj.url = url \n     jsonObj.LINKTYPE = API.Data.LINKTYPE \n\n     // Template parameters \n     jsonObJ.OfficialaccountParam = json.stringify \n } \n // WeChat Mini Program \n if.data.sendchannel == '60') {{ \n     jsonObj.page = api.data.page \n     jsonObj.templateid = api.data.templateid \n     // Template parameters \n     jsonObj.miniPramparam = json.stringify (API.Data.minIPROGramparam [0]) \n } \n\n // Nailing Robot \n if.data.sendchannel == '80') {{ \n     // Text type (Text) \n     if (seeType == '10') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n     } \n     // Graphic type (FeedCard) \n     if (seeType == '40') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.FeedCards = json.stringify (API.Data.Feedcards) \n     } \n     // Markdown Type (Markdown) \n     if (seeType == '80') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.title = Title \n     } \n     // Link type (LINK) \n     if (seeType == '110') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.title = Title \n         jsonObj.picurl = picurl \n         jsonobj.url = url \n     } \n     // Card jumping (ActionCard) \n     if (seeType == '120') { \n         jsonObj.title = Title \n         jsonObj.Content = Content \n         jsonObj.btnOrientation = api.data.btnorientation \n         jsonobj.btns = json.stringify (API.Data.bTNS) \n         jsonObj.SENDTYPE = Sendtype \n     } \n } \n // Nailing work message \n if.data.sendchannel == '90') {{ \n     // Text type \n     if (seeType == '10') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n     } \n     // voice type \n     if (seeType == '20') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.Duration = API.Data.Duration \n         jsonObj.Mediaid = API.Data.Mediaid \n     } \n     // File type || Picture type \n     if (Sendtype == '60' || Sendtype == '100') { \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.Mediaid = API.Data.Mediaid \n     } \n     // link type \n     if (seeType == '110') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.title = Title \n         jsonObj.Mediaid = API.Data.Mediaid \n         jsonobj.url = url \n     } \n     // Markdown Type (Markdown) \n     if (seeType == '80') { \n         jsonObj.Content = Content \n         jsonObj.SENDTYPE = Sendtype \n         jsonObj.title = Title \n     } \n     // Card jumping (ActionCard) \n     if (seeType == '120') { \n         jsonObj.title = Title \n         jsonObj.Content = Content \n         jsonObj.btnOrientation = api.data.btnorientation \n         jsonobj.btns = json.stringify (API.Data.bTNS) \n         jsonObj.SENDTYPE = Sendtype \n     } \n    \n     // OA (ACTIONCARD) \n     if (seeType == '130') { \n        \n         // head \n         var head = {} \n         head.bgcolor = api.data.dingdingoaheadbgcolor \n         head.text = api.data.dingdingoaheadtitle \n         jsonobj.dingdingoahead = head \n        \n         // Body \n         var body = {} \n         body.tital = api.data.di",
        "data": {
